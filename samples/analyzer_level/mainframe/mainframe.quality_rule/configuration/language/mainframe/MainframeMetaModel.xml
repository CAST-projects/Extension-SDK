<?xml version="1.0" encoding="utf-8" ?>
<metaModel file_level="client" file_no="1">
    <!-- 
    Ids are between 2001000 and 2001999
    -->
    <!-- 
    CAST_MetricAssistant_WithMetrics_Cobol is the CAIP category used for quality rules on programs
    -->
    <partialCategory name="CAST_MetricAssistant_WithMetrics_Cobol">
        <inheritedCategory name="MyCompany_COBOL_Rules"/>
    </partialCategory>

    <!-- choose a name that will not clash -->
    <category name="MyCompany_COBOL_Rules" rid="0">
        <description></description>
        <!-- add new properties here, one for each quality rule -->
        
        <property name="paragraphRedefinition" type="integer" merge="sum" rid="0">
            <description>Redefinition of paragraph</description>
            <attribute name="ACCESS_APPVIEW" intValue="1"/>
            <attribute name="ACCESS_CVS" intValue="1"/>
            <attribute name="ACCESS_HTML" intValue="1"/>
            <attribute name="INF_TYPE" intValue="2001000"/>
            <attribute name="INF_SUB_TYPE" intValue="0"/>
        </property>
        
        <property name="useBinaryForDepending" type="integer" merge="sum" rid="1">
            <description>DEPENDING variable should be BINARY</description>
            <attribute name="ACCESS_APPVIEW" intValue="1"/>
            <attribute name="ACCESS_CVS" intValue="1"/>
            <attribute name="ACCESS_HTML" intValue="1"/>
            <attribute name="INF_TYPE" intValue="2001000"/>
            <attribute name="INF_SUB_TYPE" intValue="1"/>
        </property>
        
        
    </category>

</metaModel>